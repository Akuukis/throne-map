        "@types/openlayers": "^4.6.17",
        "@types/topojson": "^3.2.2",
        "concurrently": "^6.0.0",
        "css-loader": "^5.2.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.2.0",
