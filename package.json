    "scripts": {
        "start": "yarn run watch",
        "build": "tsc -v && tsc -b tsconfig.json",
        "clean": "rm -rf coverage lib tmp tsconfig.tsbuildinfo",
        "deploy": "yarn run package && aws --profile CoE s3 --region eu-west-1 sync dist s3://coe-maps-dev --acl public-read",
        "lint": "tslint -p tsconfig.json",
        "lint:fix": "tslint -p tsconfig.json --fix",
        "lint:summary": "yarn lint:verbose | grep ERROR | sed -r 's/ERROR: \\((.*)\\)\\s\\/.*/\\1/' | sort | uniq -c | sort -nr",
        "lint:verbose": "tslint -p tsconfig.json -t verbose",
        "package": "webpack --mode production",
        "serve": "serve dist/ -p 8080",
        "watch": "webpack serve --mode development --open --hot"
    },
    "dependencies": {
        "@turf/intersect": "^6.3.0",
        "@turf/turf": "^6.3.0",
        "geojson": "^0.5.0",
